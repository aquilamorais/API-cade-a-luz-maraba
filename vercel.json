{
  "version": 2,
  "builds": [
    {
      "src": "src/main/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main/server.ts"
    }
  ],
  "env": {
    "DATABASE_URL": "@database_url",
    "JWT_SECRET": "@jwt_secret"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}
